<div class="lookup">
    <input type="text" ng-change="vm.lookup(vm.q)" ng-keydown="vm.nav($event)" ng-model="vm.q" />
    <ul>
        <li ng-repeat="o in vm.options">
            <a href="javascript:;" ng-class="{ active: vm.isSelected(o) }" ng-click="vm.select(o)" mf-lookup-option></a>
        </li>
    </ul>
</div>